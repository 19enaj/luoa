local Tab = Window:CreateTab("Games")
local Section = Tab:CreateSection("Games")
local TabUtil = Window:CreateTab("Universal")
local SectionUtil = TabUtil:CreateSection("Universal")
local TabOther = Window:CreateTab("Misc.")
local SectionOther = TabOther:CreateSection("totally random")

-------------------------------------------------------
local TabCredits = Window:CreateTab("Credits") -- Title, Image
local Paragraph = TabCredits:CreateParagraph({Title = "What Is Luoa?", Content = "Luoa is a project that includes popular games and random games in roblox its a multi script gui made by eva"})
local Paragraph = TabCredits:CreateParagraph({Title = "Who own this project?", Content = "eva and rayfieldlibrary"})
local Keybind = TabCredits:CreateKeybind({
   Name = "Close Luoa",
   CurrentKeybind = "+",
   HoldToInteract = false,
   Flag = "Keybind1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Keybind)
       
       Rayfield:Destroy()
   -- The function that takes place when the keybind is pressed
   -- The variable (Keybind) is a boolean for whether the keybind is being held or not (HoldToInteract needs to be true)
   end,
})
-------------------------------------------------------



